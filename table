import java.util.*;
class Cmdline
{
public static void main(String args[])
{
int m,n;
Scanner sc=new Scanner(System.in);
System.out.println("enter the value of m: ");
m=sc.nextInt();
System.out.println("enter the value of n: ");
n=sc.nextInt();
for(int i=1; i<=n; i++)
{
System.out.println(m+"x"+i+"="+(i*m));
}
}
}
